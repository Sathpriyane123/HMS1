{
    "builds":[
        {
            "src":"hospitalmng/wsgi.py",
            "use":"@vercel/python",
            "config":{
                "maxLamdaSize":"500mb",
                "runtime": "python3.13.2"
            }
        }
    ],
    "routes":[
        {
          "src":"/(.*)",
          "dest":"hospitalmng/wsgi.py"
        }
    ]
}